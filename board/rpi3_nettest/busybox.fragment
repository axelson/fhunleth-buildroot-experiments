# Enable more networking

CONFIG_IFPLUGD=y
CONFIG_NC=y
CONFIG_NC_SERVER=y
CONFIG_NC_EXTRA=y
CONFIG_NSLOOKUP=y
CONFIG_FEATURE_NSLOOKUP_BIG=y
CONFIG_FEATURE_NSLOOKUP_LONG_OPTIONS=y
CONFIG_NTPD=y
CONFIG_FEATURE_NTPD_SERVER=y
CONFIG_FEATURE_NTPD_CONF=y
CONFIG_PING=y
CONFIG_PING6=y
CONFIG_TRACEROUTE=y
CONFIG_TRACEROUTE6=y
CONFIG_ZCIP=y
CONFIG_UDHCPC6=y
CONFIG_UDHCPD=y

